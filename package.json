{
  "name": "prioritet",
  "version": "1.0.0",
  "description": "Portal Prioritet Group",
  "main": "server.js",
  "scripts": {
    "server": "node server",
    "server-nodemon": "nodemon server",
    "client": "npm start --prefix client",
    "start": "concurrently \"npm run client\" \"npm run server\"",
    "dev": "concurrently \"npm run server-nodemon\" \"npm run client\"",
    "build": "npm install --prefix client && npm run build --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/zXus/prioritet.git"
  },
  "keywords": [
    "nodejs",
    "mongodb",
    "mongoose",
    "rest",
    "es6+",
    "react",
    "redux",
    "jwt",
    "socket"
  ],
  "author": "Ruslan Zaripov",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/zXus/prioritet/issues"
  },
  "homepage": "https://gitlab.com/zXus/prioritet#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-validator": "^6.4.0",
    "http-proxy-middleware": "^1.0.4",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "mongoose": "^5.9.10",
    "simple-peer": "^9.7.2",
    "socket.io": "^2.3.0",
    "uuid": "^7.0.3",
    "wrtc": "^0.4.5"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.3"
  }
}
